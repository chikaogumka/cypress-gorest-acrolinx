{
  "name": "cypress-gorest-acrolinx",
  "version": "1.0.0",
  "description": "Test automation for gorest endpoints using cypress",
  "main": "index.js",
  "scripts": {
    "test": "npx cypress open",
    "test:headless": "npx cypress run",
    "test:user:id:post:api": "npx cypress run --spec cypress/tests/api-test/users_id_posts_api.cy.js",
    "test:user:api": "npx cypress run --spec cypress/tests/api-test/users_api.cy.js",
    "test:post:api": "npx cypress run --spec cypress/tests/api-test/posts_api.cy.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^13.13.2",
    "cypress-mochawesome-reporter": "^3.8.2"
  }
}
